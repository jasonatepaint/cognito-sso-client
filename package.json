{
  "name": "@jasonatepaint/cognito-sso-client",
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/jasonatepaint/cognito-sso-client/issues"
  },
  "homepage": "https://github.com/jasonatepaint/cognito-sso-client#readme",
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint src",
    "build-old": "babel src --extensions \".ts\" --out-dir dist && webpack",
    "build:pack": "node esbuild.config.js",
    "build:types": "tsc --project tsconfig.build.json && npm run lint",
    "build": "npm run build:pack && npm run build:types",
    "test": "node_modules/jest/bin/jest.js --all --silent",
    "verify-webpack": "npm run build && cp dev/** dist && cd dist && DEV_SERVER_PORT=4000 npx dev-server"
  },
  "dependencies": {
    "crypto-browserify": "3.12.0",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.5"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.21.4",
    "@types/crypto-js": "4.2.2",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "29.7.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "esbuild": "0.20.2",
    "esbuild-analyzer": "0.2.0",
    "esbuild-node-externals": "1.13.0",
    "eslint": "^8.37.0",
    "eslint-config-strongloop": "^2.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-progress-bar-reporter": "^1.0.25",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonatepaint/cognito-sso-client.git"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
